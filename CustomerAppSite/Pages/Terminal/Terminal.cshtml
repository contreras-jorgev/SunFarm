@page
@addTagHelper *, ASNA.QSys.Expo.Tags
@model ASNA.Pages.TerminalPage.Terminal
@{
    Layout = null;
}

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, minimum-scale=1.0, maximum-scale=1.0" />
    <meta name="google" content="notranslate">
    <title>Terminal</title>

    <link rel="stylesheet" href="~/lib/asna-expo/css/5250.css" />
</head>

<body>
    <form method="post">

        <Terminal5250 For=@Model />

    </form>
    <script type="module">
        import { Terminal } from '../lib/asna-expo/js/bterm/terminal.js';

        Terminal.init()
            .then(
                () => Terminal.render( () => { return { width: window.innerWidth, height: window.innerHeight }; }
            )
        );
    </script>
</body>
</html>

